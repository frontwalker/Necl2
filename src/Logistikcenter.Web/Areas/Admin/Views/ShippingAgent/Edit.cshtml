@model Logistikcenter.Web.Areas.Admin.Models.ShippingAgentModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>

<form action="/Admin/ShippingAgent/Edit" method="post">
    
    @Html.HiddenFor(m => m.Id)

    <div class="editor-label">@Html.LabelFor(m => m.Username)</div>
    <div class="editor-field">@Html.EditorFor(m => m.Username, Model.Username)</div>
    
    <div class="editor-label">@Html.LabelFor(m => m.CompanyName)</div>
    <div class="editor-field">@Html.EditorFor(m => m.CompanyName, Model.CompanyName)</div>

    <div class="editor-label">@Html.LabelFor(m => m.FirstName)</div>
    <div class="editor-field">@Html.EditorFor(m => m.FirstName, Model.FirstName)</div>

    <div class="editor-label">@Html.LabelFor(m => m.LastName)</div>
    <div class="editor-field">@Html.EditorFor(m => m.LastName, Model.LastName)</div>
    
    <input type="submit" name="Create" value="Uppdatera" />

</form>